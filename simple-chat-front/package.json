{
  "name": "simple-chat-front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@pseudomera/date-parser": "^1.0.3",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "audit": "0.0.6",
    "audit-ci": "^3.1.1",
    "axios": "^0.21.1",
    "cypress": "^6.2.1",
    "enzyme": "^3.11.0",
    "jsinspect": "^0.12.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.14.1",
    "react-icons": "^4.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-scroll-to-bottom": "^4.1.0",
    "socket.io-client": "^2.3.0",
    "socket.io-mock": "^1.3.1",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:nowatch": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint src/",
    "audit": "audit-ci --high",
    "inspect": "jsinspect -t 20 ./src",
    "cypress:open": "cypress open"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "eslint-plugin-cypress": "^2.11.2"
  }
}
